<h1>Editing Artist #<%= @artist.id %></h1>

<form action="/artists/<%= @artist.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @artist.name  %>" type="text" />
  </div>
  <div>
    <label for="nationality">nationality</label><br />
    <input id="nationality" name="nationality" value="<%= @artist.nationality  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


